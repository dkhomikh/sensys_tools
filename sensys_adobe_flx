		Case "Microradar"
			SelectItem 2
		Case "Stop Bar"
			SelectItem 4
		Case "Travel Time"
			SelectItem 5
	End Select
	If aChanges Then
		wait_custom 3: ApplyChanges
	End If
	functionend "SensorZoneTypeChange"
End Sub

